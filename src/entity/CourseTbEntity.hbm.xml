<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.CourseTbEntity" table="course_tb" schema="course_selection_system">
        <id name="courseId">
            <column name="course_id" sql-type="int(11)"/>
        </id>
        <property name="courseName">
            <column name="course_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="courseCredit">
            <column name="course_credit" sql-type="smallint(6)"/>
        </property>
        <property name="courseTime">
            <column name="course_time" sql-type="smallint(6)"/>
        </property>
        <property name="courseLimit">
            <column name="course_limit" sql-type="smallint(6)"/>
        </property>
    </class>
</hibernate-mapping>