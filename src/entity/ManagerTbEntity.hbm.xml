<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ManagerTbEntity" table="manager_tb" schema="course_selection_system">
        <id name="managerId">
            <column name="manager_id" sql-type="int(11)"/>
        </id>
        <property name="managerName">
            <column name="manager_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="managerSex">
            <column name="manager_sex" sql-type="char(1)" length="1"/>
        </property>
        <property name="managerPasswd">
            <column name="manager_passwd" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>