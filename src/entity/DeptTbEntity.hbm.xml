<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.DeptTbEntity" table="dept_tb" schema="Course_Selection_System">
        <id name="deptId">
            <column name="dept_id" sql-type="int(11)"/>
        </id>
        <property name="deptName">
            <column name="dept_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="deptDeanId">
            <column name="dept_dean_id" sql-type="int(11)"/>
        </property>
        <property name="deptTel">
            <column name="dept_tel" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>