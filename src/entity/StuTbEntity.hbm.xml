<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.StuTbEntity" table="stu_tb" schema="Course_Selection_System">
        <id name="stuId">
            <column name="stu_id" sql-type="int(11)"/>
        </id>
        <property name="majorId">
            <column name="major_id" sql-type="int(11)"/>
        </property>
        <property name="stuName">
            <column name="stu_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="stuSex">
            <column name="stu_sex" sql-type="char(1)" length="1"/>
        </property>
        <property name="stuPasswd">
            <column name="stu_passwd" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>