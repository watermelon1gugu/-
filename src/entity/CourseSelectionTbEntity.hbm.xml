<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.CourseSelectionTbEntity" table="course_selection_tb" schema="Course_Selection_System">
        <id name="stuCourseId">
            <column name="stu_course_id" sql-type="int(11)"/>
        </id>
        <property name="stuId">
            <column name="stu_id" sql-type="int(11)"/>
        </property>
        <property name="courseId">
            <column name="course_id" sql-type="int(11)"/>
        </property>
        <property name="grade">
            <column name="grade" sql-type="smallint(6)"/>
        </property>
    </class>
</hibernate-mapping>